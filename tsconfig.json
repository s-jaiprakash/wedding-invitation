{
  "compilerOptions": {
    "target": "ES2020",
    "module": "None",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./",
    "rootDir": "./src",
    "declaration": false,
    "noEmit": false,
    "removeComments": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
